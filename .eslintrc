{
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "no-console": "off",
    "indent": [ "error", 2 ],
    "quotes": [ "error", "single" ],
    "semi": ["error", "always"],
    "linebreak-style": [ "error", "unix" ],
    "no-unused-vars": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "jasmine": true
  },
  "plugins":
    ["react"],
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true,
    "impliedStrict": true,
    "templateStrings": true,
    "jsx": true,
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
